import java.util.Scanner;

class Student {
    private String first_name;
    private String middle_name;
    private String last_name;
    private String suffix;

    public String getFirstName() {
        return first_name;
    }

    public void setFirstName(String first_name) {
        this.first_name = first_name;
    }

    public String getMiddleName() {
        return middle_name;
    }

    public void setMiddleName(String middle_name) {
        this.middle_name = middle_name;
    }

    public String getLastName() {
        return last_name;
    }

    public void setLastName(String last_name) {
        this.last_name = last_name;
    }

    
    public String getSuffix() {
        return suffix;
    }

    public void setSuffix(String suffix) {
        this.suffix = suffix;
    }

   public static void main(String[] args){
       
       Scanner sc = new Scanner(System.in);
       Student prsn = new Student();
       
             
       System.out.println("\nEnter your First Name: ");
       prsn.setFirstName(sc.nextLine());

       System.out.println("Enter your Middle Name: ");
       prsn.setMiddleName(sc.nextLine());

       System.out.println("Enter your Last Name: ");
       prsn.setLastName(sc.nextLine());

       System.out.println("Enter your Suffix: ");
       prsn.setSuffix(sc.nextLine());

       
       System.out.println("First Name: "+ prsn.getFirstName());
       System.out.println("Middle Name: "+ prsn.getMiddleName());
       System.out.println("Last Name: "+ prsn.getLastName());
       System.out.println("Suffix: "+ prsn.getSuffix());

       System.out.println("Full Name: "+ prsn.getFirstName() + " " + prsn.getMiddleName() + " " + prsn.getLastName() + " " + prsn.getSuffix());
   
   
   
   
   
   }
}
